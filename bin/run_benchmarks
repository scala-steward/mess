#!/usr/bin/env bash

if ! type jenv > /dev/null 2>&1; then exit 1; fi

cd $(dirname $(pwd)/..)

date=`date +%FT%T`

for jv in "11" "12"; do
  for sv in "2.12.8" "2.13.0-M5"; do
    eval "$(jenv sh-shell $jv)"
    java -version
    sbt ++$sv clean "benchmark/jmh:run -prof gc -rf json -rff benchmark-$jv-$sv-$date.json"
  done
done
